<doctrine-mapping>
    <entity name="App\User\Permission\Model\UserPermission" table="permission">
        <id name="id" type="integer">
            <generator strategy="AUTO"/>
        </id>

        <field name="name" type="string" unique="true" length="50"/>
        <field name="updatedAt" type="datetime_immutable" nullable="true"/>
        <field name="createdAt" type="datetime_immutable"/>
    </entity>
</doctrine-mapping>